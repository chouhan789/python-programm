class student:
    def __init__(self,a,b,c):
        self.name=a
        self.rollno=b
        self.marks=c

    def printvalue(self):
        print("name=",self.name)
        print("rollno=",self.rollno)
        print("marks=",self.marks)
    def evaluate(self):
        if(self.marks>90):
            print("a grade")
        elif(self.marks>80 and self.marks<90):
            print("b grade")
        elif(self.marks>70 and self.marks<80):
            print("c grade")
s1=student("adarsh",14,97)
s1.printvalue()
s1.evaluate()
            
